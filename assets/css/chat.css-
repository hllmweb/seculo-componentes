*, *::before, *::after{
	padding:0;
	margin:0;
	box-sizing: border-box;
}
html, body{
	font-family: 'Segoe UI', 'Arial';
	font-style: normal;
	font-size:100%;
	box-sizing: border-box;
	height: 100vh;
}
body{
	background-color: #E9EBED;
	font-size:1rem;
}
h5{
	font-size:0.6rem !important;
	font-weight: bold;
	letter-spacing: 1px;
}
.content{
	/*height: 75% !important;
	height: 75% !important;*/
    /*overflow: auto;*/
    /*background-color:red;*/
    position:relative;
	display: block;
	box-sizing: border-box;
	font-size:0rem;
}
#content,
#footer,
#footer-top{
	display: block;
}
#header{
	/*height: 8%;*/
	background-color: #1A2D44;
	font-size:0;
	box-sizing: border-box;
	position: fixed;
	width: 100%;
}
#content{
	/*height:92%;*/
	height: 75%;
    position:relative;
    top: 10%;
	overflow: auto;
	display: block;
	box-sizing: border-box;
	font-size:0rem;
}
#content-100{
	height: 92%;
	position: relative;
	z-index: 0;
	display: block;
	font-size:0;
	box-sizing: border-box;
	background-color: #E9EBED;
}
#footer{
	height: 15%;
	width: 100%;
	position: absolute;
	bottom: 0;
	padding:10px 15px;
	background-color: transparent;
}
#footer-top{
	width: 100%;
	padding: 10px 15px;
	position: absolute;
	/*bottom: 200px;*/
	bottom: 0;
	background-color: #E8E4E4;
	border-bottom: 1px solid #dbd8d8;
}
.top{
	position: absolute !important;
	bottom: 50% !important;
}
#header .title{
	padding:15px;
	margin:0;
	font-size:1rem;
	color:#FFF;
	width: 80%;
	white-space: nowrap; 
    overflow: hidden;
    text-overflow: ellipsis;
}
#header .back{
	padding:10px 10px 10px 0px;
	margin:0;
}
#header .back a{
	font-size: 1.5rem;
	width: 100%;
	display: block;
	text-align: right;
	color:#FFCB31;
	font-weight: 700;
	text-decoration: none;
}
.col-80,
.col-20{
	display: inline-block;
	vertical-align: top;
}
.col-80{
	width: 80%; 
}
.col-20{
	width: 20%;
}
#content .title{
	padding:15px;
	margin:0;
	font-size:1rem;
	color:#1A2D44;
	font-weight: 700;
}
/*#footer::before{
	padding-top:20px;
}*/
.shadow{
	-webkit-box-shadow: 0px -27px 20px 0px rgba(235,235,235,0.8);
	-moz-box-shadow: 0px -27px 20px 0px rgba(235,235,235,0.8);
	box-shadow: 0px -27px 20px 0px rgba(235,235,235,0.8);
}
.title,
.data{
	display: inline-block;
}
.title{
	padding:15px;
	margin:0;
	font-size:1rem;
	color:#FFF;
}
.data{
	font-size:0.8rem;
	color:#FFCB31;
}
.item{
	width: 100%;
	display: inline-block;
}
.meu,
.outros{
	border-radius:12px;
	-webkit-box-shadow: 2px 2px 1px 0px rgba(224,224,224,1);
	-moz-box-shadow: 2px 2px 1px 0px rgba(224,224,224,1);
	box-shadow: 2px 2px 1px 0px rgba(224,224,224,1);
	font-size:0.9rem;
	position: relative;
	display: block;
}
.meu{
	background-color: #FFFFFF;
	padding:10px;
	margin:15px 20px;
	display: inline-block;
	/*width: 60%;*/
	/*float:left;*/
	border-top-left-radius: 0px !important;
}
.meu span,
.outros span{
	padding-bottom: 10px;
	display: block;
}
.outros{
	background-color: #f7f8c7;
	color:#1A2D44;
	padding:10px;
	margin:5px 20px;
	display: inline-block;
	/*width: 60%;*/
	float:right;
	border-top-right-radius: 0px !important;
}
.location{
	font-size:0.6rem;
	text-align: right;
	position: relative;
	display: block;
	/*right: 10px;*/
	margin:0px 10px;
	bottom:2.5px;
	color:#bcbcbc;
	letter-spacing: 1px;
}
.clear{
	clear: both;
}
.row{
	width: 100%;
}
.list{
	list-style: none;
	margin: 0 10px;
	border-radius:8px;
	background-color: #FFF;
}
.list-item{
	padding:0;
	margin:0;
	width: 100%;
}
.list-link{
	display: block;
	width: 100%;
	border-bottom: 1px solid #DDD;
	padding:10px 15px;
	text-align: left;
	font-size:1rem;
	color:#1A2D44;
	text-transform: uppercase;
	text-decoration: none;
	font-weight: 700;		
}
.list-user{
	list-style: none;
	background-color: #FFF;
}
.list-user-item{
	padding:0;
	margin:0;
	width: 100%;
}
.list-user-link{
	display: block;
	width: 100%;
	border-bottom: 1px solid #DDD;
	padding:10px 15px;
	text-align: left;
	font-size:1rem;
	color:#1A2D44;
	text-transform: uppercase;
	text-decoration: none;
	font-weight: 700;		
	position: relative;
}
.text-link{
	width: 80%;
}
.badget{
	position: absolute;
	right: 10px;
	background-color: #CC0000;
	font-size:0.8rem;
	padding:2px 5px;
	color:#FFF;
	border-radius:12px;
}
.activo{
	background-color: #f7f8c7;
}
.enable{
	display: block !important;
}
.disable{
	display: none !important;
}
.flex{
	display: flex;
	flex-direction: row wrap;
	flex-flow: row;
	justify-content: center;
	align-items: center;
}
.item-flex{
	background-color: transparent;
}
.item-flex:nth-child(1){
	flex:1 0 100% !important;
	position: relative;
}
.item-flex:nth-child(2){
	/* flex:1 0 20% !important; */
	vertical-align: middle;
	text-align: center;
	position: absolute;
	right: 20px;
}
.btn_esconder{
	border-radius:50px;
	width: 50px;
	height: 50px;
	margin-left:5px;
	background-color: #3c70af;
	color:#FFF;
	font-weight: bold;
	vertical-align: middle;
	border:0;
	outline: 0;
	cursor: pointer;
}
.btn_esconder i{
	font-size:1.5rem;
}

.txt_mensagem,
.btn_enviar{
	display: inline-block;
	padding:0;
	margin:0;
	box-sizing: border-box;
	vertical-align: middle;
	width: 100%;
	height: 100%;
	border:0;
	outline: 0;
	cursor: pointer;
}
.txt_mensagem{
	border-radius:50px;
	height: 65px;
	padding:10px 15px;
	font-family: 'Segoe UI', 'Arial';
	font-size:0.8rem;
	padding-right: 15%;
}

.btn_enviar{
	border-radius:50px;
	width: 100px;
	height: 50px;
	margin-left:5px;
	background-color: #FFCB31;
	color:#1A2D44;
	font-weight: bold;
	font-size:1rem;
}
.bg_body{
	background:url("../images/bg_noite.jpg") no-repeat;
	background-size: cover;
}
