.col-30,
.col-70{
	height: 100%;
	display: inline-block;
	vertical-align: top;
	padding:0;
	margin:0;
	font-size:1rem;
}
.col-30{
	width: 30%;
	height: 100%;
	background-color: #FFF;
}
.col-70{
	width: 70%;
	position: relative;
}